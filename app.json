{
  "expo": {
    "name": "AmiCare",
    "slug": "AmiCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "AmiCare",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to the camera for identity verification purposes."
      }
    },
    "android": {
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
