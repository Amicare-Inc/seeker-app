{
  "expo": {
    "name": "Amicare",
    "slug": "AmiCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "amicare",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A8BF8"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to the camera for identity verification purposes.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.amicare.app",
      "teamId": "5284JKTY8S",
      "merchantId": "merchant.com.amicare.app"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "CAMERA"
      ],
      "package": "com.amicare.AmiCare"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.amicare.app",
          "enableGooglePay": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5d43b14b-410b-4ea2-bf03-3cc6de49683a"
      }
    },
    "owner": "amicare"
  }
}